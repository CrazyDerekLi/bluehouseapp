{% extends "BlackhouseappBluehouseappBundle:layout:layout.html.twig" %}
{% block body -%}
    <h1>Baned IP</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Ip</th>
                <td>{{ entity.ip }}</td>
            </tr>
            <tr>
                <th>Timestamp</th>
                <td>{{ entity.timestamp|date("Y-m-d H:i") }}</td>
            </tr>
            <tr>
                <th>Fromdate</th>
                <td>{{ entity.fromDate|date("Y-m-d H:i") }}</td>
            </tr>
            <tr>
                <th>Todate</th>
                <td>{{ entity.toDate|date("Y-m-d H:i") }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_banedips') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_banedips_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
